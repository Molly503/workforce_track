      <aliases enabled='yes' />
      <column caption='Monthly Hires' datatype='integer' name='[Calculation_1949566064385380352]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='// 计算字段名：Monthly Hires Count&#10;{COUNTD(IF MONTH([hire_date]) = MONTH([Parameters].[Parameter 2])&#10;        AND YEAR([hire_date]) = YEAR([Parameters].[Parameter 2])&#10;     THEN [employee_id] END)}' />
      </column>
      <column caption='Monthly Terminations' datatype='integer' name='[Calculation_1949566064386199553]' role='dimension' type='ordinal'>
        <calculation class='tableau' formula='{COUNTD(IF NOT ISNULL([termination_date]) &#10;        AND MONTH([termination_date]) = MONTH([Parameters].[Parameter 2]) &#10;        AND YEAR([termination_date]) = YEAR([Parameters].[Parameter 2]) &#10;     THEN [employee_id] END)}' />
      </column>
      <column caption='Total Headcount' datatype='integer' name='[Calculation_1949566064386355202]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='SUM(IF ISNULL([termination_date]) OR [termination_date] &gt; TODAY() THEN 1 ELSE 0 END)' />
      </column>
      <column caption='Turnover Score' datatype='real' name='[Calculation_1949566064386854916]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='-3.769022 * [satisfaction] + 0.207596 * [evaluation] + 0.170145 * [years_at_company] + 0.181896' />
      </column>
      <column caption='Risk Classification' datatype='string' name='[Calculation_1949566064387612677]' role='dimension' type='nominal'>
        <calculation class='tableau' formula='// 计算字段：Risk Classification&#10;IF [Calculation_1949566064386854916] &gt; 0.90 THEN &quot;High Risk&quot;&#10;ELSEIF [Calculation_1949566064386854916] &gt; 0.60 THEN &quot;Medium Risk&quot;&#10;ELSEIF [Calculation_1949566064386854916] &gt; 0.20 THEN &quot;Low Risk&quot;&#10;ELSE &quot;Safe&quot;&#10;END' />
      </column>
      <column caption='Recent Leavers' datatype='integer' name='[Calculation_1949566064387698694]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='IF NOT ISNULL([termination_date]) AND [termination_date] &gt;= DATEADD(&apos;day&apos;, -30, TODAY()) AND [termination_date] &lt;= TODAY() THEN 1 ELSE 0 END' />
      </column>
      <column caption='YTD Cumulative Hires' datatype='integer' name='[Calculation_1949566064406462471]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='SUM(IF [hire_date] &gt;= DATEADD(&apos;year&apos;, 0, DATETRUNC(&apos;year&apos;, [Parameters].[Parameter 2])) &#10;    AND [hire_date] &lt;= [Parameters].[Parameter 2] THEN 1 ELSE 0 END)' />
      </column>
      <column caption='YTD Cumulative Terminations' datatype='integer' name='[Calculation_1949566064406560776]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='SUM(IF [termination_date] &gt;= DATEADD(&apos;year&apos;, 0, DATETRUNC(&apos;year&apos;, [Parameters].[Parameter 2])) &#10;    AND [termination_date] &lt;= [Parameters].[Parameter 2] THEN 1 ELSE 0 END)' />
      </column>
      <column caption='Monthly Turnover Rate' datatype='real' name='[Calculation_6114973508593905666]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='// Monthly Turnover Rate (不乘以100)&#10;(&#10;{COUNTD(IF NOT ISNULL([termination_date])&#10;        AND MONTH([termination_date]) = MONTH([Parameters].[Parameter 2])&#10;        AND YEAR([termination_date]) = YEAR([Parameters].[Parameter 2])&#10;     THEN [employee_id] END)}&#10;    /&#10;{COUNTD(IF [hire_date] &lt; DATEADD(&apos;day&apos;, 1, DATETRUNC(&apos;month&apos;, [Parameters].[Parameter 2]))&#10;        AND (ISNULL([termination_date]) OR [termination_date] &gt;= DATETRUNC(&apos;month&apos;, [Parameters].[Parameter 2]))&#10;     THEN [employee_id] END)}&#10;)' />
      </column>
      <column caption='Termination Year' datatype='integer' name='[Calculation_6114973508618117127]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='// 计算字段：Termination Year&#10;IF NOT ISNULL([termination_date]) THEN &#10;    YEAR([termination_date])&#10;ELSE &#10;    NULL&#10;END' />
      </column>
      <column caption='Termination Month' datatype='integer' name='[Calculation_6114973508618190856]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='MONTH([termination_date])' />
      </column>
      <column caption='Selected Month Display' datatype='string' name='[Calculation_6114973508626870285]' role='dimension' type='nominal'>
        <calculation class='tableau' formula='STR(YEAR([Parameters].[Parameter 2])) + &quot;-&quot; + RIGHT(&quot;0&quot; + STR(MONTH([Parameters].[Parameter 2])), 2)' />
      </column>
      <column caption='Is Selected Month' datatype='boolean' name='[Calculation_6114973508628656142]' role='dimension' type='nominal'>
        <calculation class='tableau' formula='YEAR([termination_date]) = YEAR([Parameters].[Parameter 2])&#10;AND MONTH([termination_date]) = MONTH([Parameters].[Parameter 2])' />
      </column>
      <column caption='Is Terminated in Selected Month' datatype='boolean' name='[Calculation_6114973508634345487]' role='dimension' type='nominal'>
        <calculation class='tableau' formula='NOT ISNULL([termination_date])&#10;AND MONTH([termination_date]) = MONTH([Parameters].[Parameter 2])&#10;AND YEAR([termination_date]) = YEAR([Parameters].[Parameter 2])' />
      </column>
      <column caption='Is 2025 Termination' datatype='boolean' name='[Calculation_6114973508636491792]' role='dimension' type='nominal'>
        <calculation class='tableau' formula='YEAR([termination_date]) = 2025' />
      </column>
      <column caption='Termination Month Year' datatype='string' name='[Calculation_6114973508636577809]' role='dimension' type='nominal'>
        <calculation class='tableau' formula='IF NOT ISNULL([termination_date]) THEN &#10;    STR(MONTH([termination_date])) + &quot;/&quot; + STR(YEAR([termination_date]))&#10;ELSE &#10;    &quot;No Date&quot;&#10;END' />
      </column>
      <column caption='Termination Debug Detail' datatype='string' name='[Calculation_6114973508636651538]' role='dimension' type='nominal'>
        <calculation class='tableau' formula='IF ISNULL([termination_date]) THEN&#10;    &quot;No Term Date&quot;&#10;ELSE&#10;    &quot;Term Date: &quot; + STR(MONTH([termination_date])) + &quot;/&quot; + STR(YEAR([termination_date])) + &#10;    &quot; | Selected: &quot; + STR(MONTH([Parameters].[Parameter 2])) + &quot;/&quot; + STR(YEAR([Parameters].[Parameter 2])) +&#10;    &quot; | Match: &quot; + &#10;    IF MONTH([termination_date]) = MONTH([Parameters].[Parameter 2]) AND YEAR([termination_date]) = YEAR([Parameters].[Parameter 2])&#10;    THEN &quot;YES&quot; ELSE &quot;NO&quot; END&#10;END' />
      </column>
      <column caption='Yearly Terminations' datatype='integer' name='[Calculation_6114973508637212691]' role='measure' type='quantitative'>
