      <column caption='Employees at the Beginning of the Year' datatype='integer' name='[Calculation_6114973508673667093]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='COUNTD(IF [hire_date] &lt;= DATETRUNC(&apos;year&apos;, [Parameters].[Parameter 2]) &#10;       AND (ISNULL([termination_date]) OR [termination_date] &gt; DATETRUNC(&apos;year&apos;, [Parameters].[Parameter 2]))&#10;    THEN [employee_id] &#10;END)' />
      </column>
      <column caption='Selected Year Display' datatype='string' name='[Calculation_6114973508674097174]' role='dimension' type='nominal'>
