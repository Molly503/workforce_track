      <column caption='Yearly Hires' datatype='integer' name='[Calculation_6114973508674600983]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='{COUNTD(IF YEAR([hire_date]) = YEAR([Parameters].[Parameter 2]) &#10;     THEN [employee_id] &#10;END)}' />
